%table
  %tr
    %th Name:
    %td= applicant.name
  %tr
    %th Email:
    %td= applicant.email
  %tr
    %th Education:
    %td= render partial: 'educations/education', collection: applicant.education
  %tr
    %th Experience:
    %td= render partial: 'experiences/experience', collection: applicant.experiences
  %tr
    %th Achievements:
    %td= render partial: 'achievements/achievement', collection: applicant.achievements
